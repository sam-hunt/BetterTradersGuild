<?xml version="1.0" encoding="utf-8"?>
<Defs>
  <!--
    Workshop - Fabrication Bench with Stool

    Design (wall at top/z=2):
        x: 0 1 2 3 4
    z=2:   F F F F F   (fabrication bench back)
    z=1:   F F F F F   (fabrication bench front)
    z=0:   . . S . .   (stool at interaction spot)
  -->
  <PrefabDef>
    <defName>BTG_FabricationBench_Edge</defName>
    <edgeOnly>true</edgeOnly>
    <size>(5,3)</size>
    <things>
      <FabricationBench>
        <position>(0, 0, 1)</position>
        <relativeRotation>Opposite</relativeRotation>
      </FabricationBench>
      <Stool>
        <position>(2, 0, 0)</position>
        <stuff>Steel</stuff>
      </Stool>
    </things>
  </PrefabDef>
</Defs>
