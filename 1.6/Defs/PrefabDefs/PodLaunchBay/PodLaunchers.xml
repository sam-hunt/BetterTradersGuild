<?xml version="1.0" encoding="utf-8"?>
<Defs>
  <!--
    Better Traders Guild - Double Pod Launcher Prefab
    Interior placement (not edge-only) for central positioning in launch bay.
  -->
  <PrefabDef>
    <defName>BTG_PodLaunchers</defName>
    <size>(3,3)</size>
    <things>
      <TransportPod>
        <rects>
          <li>(0,1,0,1)</li>
          <li>(1,2,1,2)</li>
        </rects>
      </TransportPod>
      <FirefoamPopper>
        <position>(2, 0, 0)</position>
      </FirefoamPopper>
      <PodLauncher>
        <positions>
          <li>(1, 0, 1)</li>
          <li>(2, 0, 2)</li>
        </positions>
        <relativeRotation>Opposite</relativeRotation>
      </PodLauncher>
    </things>
  </PrefabDef>

</Defs>