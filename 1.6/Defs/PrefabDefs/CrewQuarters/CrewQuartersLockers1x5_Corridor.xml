<?xml version="1.0" encoding="utf-8"?>
<Defs>
  <!--
    Better Traders Guild - Crew Quarters Corridor Locker Prefab

    Features:
    - 1x5 prefab size
    - Decorative filler for short subroom strips which can't quite
      filled with available subroom size variants

    Design (top view, North is up, entry from z=0):
        x: 0
    z=4:   P
    z=3:   L
    z=2:   L
    z=1:   L
    z=0:   P

    Legend:
    P = PlantPot (steel)
    L = AncientLockerBank (facing East)
    . = empty space
  -->
  <PrefabDef>
    <defName>BTG_CrewQuartersLockers1x5_Corridor</defName>
    <size>(1,5)</size>
    <things>
      <AncientLockerBank>
        <positions>
          <li>(0, 0, 2)</li>
        </positions>
        <relativeRotation>Clockwise</relativeRotation>
      </AncientLockerBank>
      <PlantPot>
        <positions>
          <li>(0, 0, 0)</li>
          <li>(0, 0, 4)</li>
        </positions>
        <stuff>Steel</stuff>
        <quality>Normal</quality>
      </PlantPot>
    </things>
  </PrefabDef>
</Defs>