<?xml version="1.0" encoding="utf-8"?>
<Defs>
  <!--
    Better Traders Guild - Crew Quarters Large Stove Prefab
    Requires VFE Production for the bench.

    Features:
    - 2x5 prefab size

    Design (top view, North is up, all facing East):
        x: 0  1
    z=4:   S  S    S = VFE_TableStoveLarge (5x2 facing East)
    z=3:   S  S
    z=2:   S  S
    z=1:   S  S
    z=0:   S  S

    Legend:
    S = VFE_TableStoveLarge (5x2 rotated East)
  -->

  <PrefabDef MayRequire="VanillaExpanded.VFEProduction">
    <defName>BTG_CrewQuartersLargeStove2x5</defName>
    <size>(2,5)</size>
    <things>
      <VFE_TableStoveLarge>
        <position>(1, 0, 2)</position>
        <relativeRotation>Counterclockwise</relativeRotation>
      </VFE_TableStoveLarge>
      <WallLamp>
        <position>(0, 0, 2)</position>
        <relativeRotation>Counterclockwise</relativeRotation>
      </WallLamp>
    </things>
  </PrefabDef>

</Defs>