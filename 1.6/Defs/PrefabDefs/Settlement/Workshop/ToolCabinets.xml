<?xml version="1.0" encoding="utf-8"?>
<Defs>
  <!--
    Workshop - Tool Cabinets (Edge)
    Facilities that link to nearby workbenches.

    Design (wall at z=0):
        x: 0 1
    z=0:   T T   (tool cabinets against wall)
  -->
  <PrefabDef>
    <defName>BTG_ToolCabinets</defName>
    <edgeOnly>true</edgeOnly>
    <size>(2,1)</size>
    <things>
      <ToolCabinet>
        <positions>
          <li>(0, 0, 0)</li>
          <li>(1, 0, 0)</li>
        </positions>
      </ToolCabinet>
    </things>
  </PrefabDef>
</Defs>
