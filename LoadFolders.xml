<?xml version="1.0" encoding="utf-8"?>
<loadFolders>
  <!--
    LoadFolders.xml - Tells RimWorld which folders to load based on game version.

    This mod requires RimWorld 1.6+ (Odyssey DLC).
    Version-specific content (Assemblies, Defs, Patches) lives in the 1.6/ folder.
    The root "/" loads About/ and other shared metadata.
  -->
  <v1.6>
    <li>/</li>
    <li>1.6</li>
  </v1.6>
</loadFolders>
